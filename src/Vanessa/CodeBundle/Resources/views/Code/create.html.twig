{% extends "VanessaCodeBundle::layout.html.twig" %}

{% block title %}Add a code{% endblock %}


{% block body %}
<div class="row">
    <div class="span12">
        <h3 class="align_header">Add a code</h3>
        <div class="span3">
           {% set vars = {'action': 'Add'} %}
           {% include 'VanessaCodeBundle::breadcrumb.html.twig' with vars  %}
            </div>
    </div>
    <div class="span12 pull-right">
            {% block alert %}
                {% include "::alert.html.twig" %}
            {% endblock %}
            <div class="span8">
                {% include "VanessaCodeBundle:Code:Form/form.create.html.twig" %} 
            </div> 
            <div class="span3 pull-right">
                {% include "VanessaCodeBundle:Code:Hint/form.create.html.twig" %} 
            </div>  
              
    </div> 
</div>
{% endblock body %}


