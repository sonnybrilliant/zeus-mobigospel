<div class="shift_30 ">
{% block alert %}
    {% include "::alert.html.twig" %}
{% endblock %}    
<form class="display helpers form-horizontal" action="" method="POST" {{ form_enctype(form) }}>
  <fieldset>
      <div class="control-group {% if form_errors(form.msisdn) %} error {% endif %}">
        <label class="control-label" for="msisdn">{{ form_label(form.msisdn) }}</label>
        <div class="controls">
          {{ form_widget(form.msisdn) }} 
          <span class="help-inline"><a href="#" rel="tooltip" data-placement="right" data-original-title="Cellphone number"><i class="icon-info-sign"></i></a></span>  
          <span class="help-inline">{{form_errors(form.msisdn)}}</span>  
        </div>
      </div> 
      <div class="control-group {% if form_errors(form.toAddress) %} error {% endif %}">
        <label class="control-label" for="to_address">{{ form_label(form.toAddress) }}</label>
        <div class="controls">
          {{ form_widget(form.toAddress) }} 
          <span class="help-inline"><a href="#" rel="tooltip" data-placement="right" data-original-title="To Address"><i class="icon-info-sign"></i></a></span>  
          <span class="help-inline">{{form_errors(form.toAddress)}}</span>  
        </div>
      </div> 
      <div class="control-group {% if form_errors(form.seqno) %} error {% endif %}">
        <label class="control-label" for="seqno">{{ form_label(form.seqno) }}</label>
        <div class="controls">
          {{ form_widget(form.seqno) }} 
          <span class="help-inline"><a href="#" rel="tooltip" data-placement="right" data-original-title="Sequence number"><i class="icon-info-sign"></i></a></span>  
          <span class="help-inline">{{form_errors(form.seqno)}}</span>  
        </div>
      </div> 
      <div class="control-group {% if form_errors(form.status) %} error {% endif %}">
        <label class="control-label" for="seqno">{{ form_label(form.status) }}</label>
        <div class="controls">
          {{ form_widget(form.status) }} 
          <span class="help-inline"><a href="#" rel="tooltip" data-placement="right" data-original-title="Message status"><i class="icon-info-sign"></i></a></span>  
          <span class="help-inline">{{form_errors(form.status)}}</span>  
        </div>
      </div> 
      <div class="control-group {% if form_errors(form.network) %} error {% endif %}">
        <label class="control-label" for="seqno">{{ form_label(form.network) }}</label>
        <div class="controls">
          {% if rxqueue.network == 1 %}
             <input type="text" value="Vodacom" disabled="disabled" class="span4" />
          {% elseif rxqueue.network == 2 %}
              <input type="text" value="MTN" disabled="disabled" class="span4" />
          {% elseif rxqueue.network == 3 %}
             <input type="text" value="Cell C" disabled="disabled" class="span4" />
          {% else %}
             <input type="text" value="Other" disabled="disabled" class="span4" />
          {% endif %}    
          <span class="help-inline"><a href="#" rel="tooltip" data-placement="right" data-original-title="Network"><i class="icon-info-sign"></i></a></span>  
          <span class="help-inline">{{form_errors(form.network)}}</span>  
        </div>
      </div> 
      <div class="control-group {% if form_errors(form.createdAt) %} error {% endif %}">
        <label class="control-label" for="seqno">{{ form_label(form.createdAt) }}</label>
        <div class="controls">
             <input type="text" value="{{rxqueue.createdAt|date('Y-m-d H:i A')}}" disabled="disabled" class="span4" />
  
          <span class="help-inline"><a href="#" rel="tooltip" data-placement="right" data-original-title="Received At"><i class="icon-info-sign"></i></a></span>  
          <span class="help-inline">{{form_errors(form.createdAt)}}</span>  
        </div>
      </div> 
      <div class="control-group {% if form_errors(form.body) %} error {% endif %}">
        <label class="control-label" for="seqno">{{ form_label(form.body) }}</label>
        <div class="controls">
          {{ form_widget(form.body) }}   
          <span class="help-inline"><a href="#" rel="tooltip" data-placement="right" data-original-title="SMS content"><i class="icon-info-sign"></i></a></span>  
          <span class="help-inline">{{form_errors(form.body)}}</span>  
        </div>
      </div> 
  </fieldset>
</form>
</div>        
{{ tinymce_init() }}
     