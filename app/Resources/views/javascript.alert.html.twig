{% if is_granted('IS_AUTHENTICATED_FULLY') %}
<script type="text/javascript">
    {% for flashMessage in app.session.flashbag.get('alert-error') %}
         alertify.log('{{ flashMessage }}','error');
    {% endfor %}
        
    {% for flashMessage in app.session.flashbag.get('alert-notice') %}
         alertify.log('{{ flashMessage }}','notice');
    {% endfor %}
    
    {% for flashMessage in app.session.flashbag.get('alert-warning') %}
         alertify.log('{{ flashMessage }}','warning');
    {% endfor %}
        
    {% for flashMessage in app.session.flashbag.get('alert-success') %}
         alertify.log('{{ flashMessage }}','success');
    {% endfor %}
        
    {% for flashMessage in app.session.flashbag.get('alert-info') %}
         alertify.log('{{ flashMessage }}','info');
    {% endfor %}
</script>     
{% endif %}  